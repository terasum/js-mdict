#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/chenquan/Workspace/nodejs/js-mdict-demo/node_modules/.pnpm/levenshtein-edit-distance@3.0.1/node_modules/levenshtein-edit-distance/node_modules:/Users/chenquan/Workspace/nodejs/js-mdict-demo/node_modules/.pnpm/levenshtein-edit-distance@3.0.1/node_modules:/Users/chenquan/Workspace/nodejs/js-mdict-demo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/chenquan/Workspace/nodejs/js-mdict-demo/node_modules/.pnpm/levenshtein-edit-distance@3.0.1/node_modules/levenshtein-edit-distance/node_modules:/Users/chenquan/Workspace/nodejs/js-mdict-demo/node_modules/.pnpm/levenshtein-edit-distance@3.0.1/node_modules:/Users/chenquan/Workspace/nodejs/js-mdict-demo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../levenshtein-edit-distance@3.0.1/node_modules/levenshtein-edit-distance/cli.js" "$@"
else
  exec node  "$basedir/../../../../../levenshtein-edit-distance@3.0.1/node_modules/levenshtein-edit-distance/cli.js" "$@"
fi
